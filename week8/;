#insert
#append
#remove
#find

class Link:
    def __init__(self, data):
        self.data = data
        self.next_link = None



class LinkedList:
    def __init__(self):
        self.start = None


    def append(self, data):
        if self.start == None:
            self.start = Link(data)
        curr = self.start
        while curr.next_link != None:
            curr = curr.next_link
        curr.next_link = Link(data):







